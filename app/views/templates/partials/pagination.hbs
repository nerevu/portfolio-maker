<ul class="pagination pagination-lg">
  <li {{#if first_page}}class="disabled"{{/if}}>
    <a href="#">&laquo;</a>
  </li>
  {{#times pages}}
    <li {{#if active}}class="active"{{/if}}>
      <a href="{{../type}}/page/{{this}}">
        {{this}} {{#if active}}<span class="sr-only">(current)</span>{{/if}}
      </a>
    </li>
  {{/times}}
  <li {{#if last_page}}class="disabled"{{/if}}>
    <a href="{{../type}}/page/{{p}}">&raquo;</a>
  </li>
</ul>
