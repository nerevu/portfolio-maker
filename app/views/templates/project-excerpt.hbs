<div id="project-excerpt-{{id}}" class="excerpt excerpt-project">
  <div class="row excerpt-head cushion">
    <div class="col-xs-12">
      <h2 class="excerpt-title"><a href="{{href}}">{{title}}</a></h2>
    </div>
    <div class="hidden-xs col-sm-6 excerpt-image">
      <a href="{{href}}">{{partial 'project-image' url_s}}</a>
    </div>
    <div class="visible-xs col-xs-12 excerpt-image cushion">
      <a href="{{href}}">{{partial 'project-image' url_s}}</a>
    </div>
    <div class="col-xs-12 col-sm-6 excerpt-meta">
      {{partial 'project-meta-list' this}}
    </div>
  </div>
  <div class="excerpt-body clearfix">
   	{{partial 'tags' this}}
    <h2 class="excerpt-sub-title"><small>{{description}}</small></h2>
  </div>
</div>


